{{ #posts }}
  <div id="{{ id }}" data-type="posts">
    <div class="thumbnail" style="background-image: url({{ acf.images }});">
      <div class="date">
        Du {{ acf.date_de_debut }} au {{ acf.date_de_debut }}
      </div>
    </div>
    <h2 class="title">{{ title.rendered }}</h2>
    <p class="description">{{{ acf.description }}}</p>
  </div>
{{ /posts }}
